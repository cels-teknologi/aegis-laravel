{
  "name": "cels-teknologi/aegis-laravel",
  "description": "Laravel exception handler",
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "guzzlehttp/guzzle": "^7.2",
    "illuminate/support": "^10.0|^11.0",
    "monolog/monolog": "^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "mockery/mockery": "^1.4.4|^1.6",
    "phpunit/phpunit": "^9.5.10|^10.1|^10.5"
  },
  "autoload": {
    "psr-4": {
      "Cels\\Aegis\\": "src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
          "Cels\\Aegis\\Tests\\": "tests/"
      }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cels\\Aegis\\AegisServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Stephen",
      "email": "stephen@cels.co.id"
    }
  ]
}

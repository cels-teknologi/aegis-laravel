{
  "name": "cels-teknologi/aegis-laravel",
  "description": "Laravel 8.x - 11.x exception handler",
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "guzzlehttp/guzzle": "^7.0.1",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^6.0|^7.0",
    "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "Cels\\Aegis\\": "src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
          "Cels\\Aegis\\Tests\\": "tests/"
      }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cels\\Aegis\\AegisServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Stephen",
      "email": "stephen@cels.co.id"
    }
  ]
}
